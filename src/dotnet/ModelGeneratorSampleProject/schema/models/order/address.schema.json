{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Address",
  "description": "주소 정보 모델",
  "type": "object",
  "properties": {
    "street": {
      "type": "string",
      "description": "도로명 주소"
    },
    "city": {
      "type": "string",
      "description": "도시"
    },
    "state": {
      "type": "string",
      "description": "주/도"
    },
    "zip_code": {
      "type": "string",
      "description": "우편번호"
    },
    "country": {
      "type": "string",
      "description": "국가"
    }
  },
  "required": ["street", "city", "zip_code", "country"]
}
